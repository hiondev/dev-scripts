#!/bin/bash
if [ "$1" = "help" ]; then
    echo "Stop the local containers"
    exit
fi

export PROJECT_ID
export SQL_CONNECTION="dummy"
export SQL_NAME="dummy"
export SQL_USER="dummy"
export SQL_PASSWORD="dummy"
export SQL_PREFIX="dummy"

docker compose --project-directory $ROOT_DIR \
-f $HIONDEV_DIR/docker-compose.yaml \
down --remove-orphans $@
